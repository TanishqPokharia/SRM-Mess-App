                        text:"Weekdays 7-9 AM/Weekends 7:30 - 9 AM"
                        font_size:24

                        IconLeftWidget:
                            icon:"clock-time-seven-outline"
                            theme_icon_color:"Custom"
                            icon_color: 0,1,0,1


