WindowManager:
    id:sm
    MenuWindow:
<MenuWindow>:
    name: "menu"

    MDFloatLayout:
        orientation: 'vertical'
        size: root.width, root.height
        MDFloatLayout:
            orientation: 'vertical'
            size: root.width,root.height

            MDTopAppBar:
                id:toolbar
                title: "SRM Mess Menu"
                right_action_items: [["dots-vertical", lambda x: root.drpdown(x)]]
                pos_hint:{"top":1}
                md_bg_color: 234/255, 232/255, 26/255, 1



            MDFloatingActionButtonSpeedDial:
                data: app.data
                root_button_anim: True
                on_open: app.open()
                on_close: app.close()
                bg_color_root_button: 37/255, 183/255, 236/255, 1
                bg_color_stack_button: 37/255, 183/255, 236/255, 1
                label_text_color: (1,1,1,1) if self.theme_cls.theme_style == 'Dark' else (0,0,0,1)


