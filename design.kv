<Item>

    ImageLeftWidget:
        source: root.source


WindowManager:
    id:sm
    MenuWindow:
<MenuWindow>:
    name: "menu"

    MDRelativeLayout:
        orientation: 'vertical'
        size: root.width, root.height
        MDFloatLayout:
            orientation: 'vertical'
            size: root.width,root.height

            MDTopAppBar:
                id:toolbar
                title: "SRM Mess Menu"
                anchor_title: "left"
                right_action_items: [["dots-vertical", lambda x: root.dropdown(x)]]
                pos_hint:{"top":1}
                md_bg_color: (200/255,161/255,3/255,1)
            MDLabel:
                id:selected_day
                halign: 'center'
                font_size:'20sp'
                text:root.current_day.capitalize()
                pos_hint:{"center_y":0.947}
                theme_text_color: "Custom"
                text_color: (1,1,1,1)
                bold:True

            MDFloatingActionButtonSpeedDial:
                id: speedDial
                data: app.data
                root_button_anim: True
                on_open:
                    app.open()
                    self.elevation_normal:0
                on_close:
                    app.close()
                    self.elevation_normal:0
                bg_color_root_button: (19/255,79/255,164/255,1)
                bg_color_stack_button: (19/255,79/255,164/255,1)
                label_text_color: (1,1,1,1) if self.theme_cls.theme_style == 'Dark' else (0,0,0,1)
                bg_color_normal: 1,0,0,1
                elevation_normal: 0





        MDCarousel:
            id: carousel
            direction: 'right'
            pos_hint:{"center_x":0.5,"center_y":0.5}
            size_hint:0.8,0.75


